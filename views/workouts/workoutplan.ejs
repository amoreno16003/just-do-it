<h1>Your workout plan!</h1>
<form method="get" action="/workouts/search">
    <button type="submit">Back to Search</button>
</form>
  <% data.forEach((elem) => { %>
    <h2><%= elem.name %> </h2>
    <h3><%= elem.target %> </h3>
    <img src=<%= elem.gifUrl %> >
    <h4><%= currentUser.id %> </h4>
    
    <form method="POST" action="/workouts/myworkoutroutine">
      <input type="text" value="<%= elem.name %>" name="name" hidden>
      <input type="text" value="<%= elem.target %>" name="target" hidden>
      <input type="text" value="<%= elem.gifUrl %>" name="gif" hidden>
      <input type="text" value="<%= currentUser.id %>" name="userId" hidden>
      <button type="submit">Add To Routine</button>
    </form>
    <br>
    <br>
 <% }) %>